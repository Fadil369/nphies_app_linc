{
  "name": "nphies-backend",
  "version": "1.0.0",
  "description": "Backend API for NPHIES integration with CopilotKit support",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t nphies-backend .",
    "docker:run": "docker run -p 3001:3001 nphies-backend"
  },
  "dependencies": {
    "@copilotkit/runtime": "^1.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.8.1",
    "zod": "^3.22.4",
    "axios": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "winston": "^3.10.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "@azure/storage-blob": "^12.15.0",
    "@azure/key-vault-secrets": "^4.7.0",
    "@azure/identity": "^4.12.0",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/jest": "^29.5.3",
    "typescript": "^5.1.6",
    "tsx": "^3.12.7",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "eslint": "^8.47.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12"
  },
  "keywords": [
    "nphies",
    "healthcare",
    "saudi-arabia",
    "copilotkit",
    "api",
    "insurance",
    "claims"
  ],
  "author": "Healthcare AI Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
